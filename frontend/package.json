		"frappe-ui": "^0.1.192",
		"lucide-vue-next": "^0.554.0",
		"pako": "^2.1.0",
		"pinia": "^3.0.4",
		"showdown": "^2.1.0",
		"socket.io-client": "^4.7.2",
		"vue": "^3.5.13",
		"vue-chartjs": "^5.3.3",
		"vue-router": "^4.5.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@vitejs/plugin-vue": "^5.1.4",
		"@vitest/coverage-v8": "^2.1.8",
		"autoprefixer": "^10.4.2",
		"jsdom": "^25.0.1",
		"postcss": "^8.4.5",
