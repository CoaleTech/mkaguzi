{
	"name": "frappe-ui-frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build --base=/assets/mkaguzi/frontend/ && yarn copy-html-entry",
		"preview": "vite preview",
		"lint": "biome check --write .",
